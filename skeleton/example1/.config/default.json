{
	"site":{
		"domain":"unknown.com"
	},
	"source":{
		"current_path":"/home/%(user)s/%(domain)s/pages/current",
		"repo_path":"/home/%(user)s/%(domain)s/pages/repo",
		"tag_path":"/home/%(user)s/%(domain)s/pages/source/%(tag)s"
	},
	"mysql_backup":{
		"db_backup_path":"/home/%(user)s/%(domain)s/pages/db_backup",
		"db":{
			"host":"localhost",
			"user":"root",
			"password":"",
			"database":""
		}
	},
	"stages":{
		"after_checkout":{
			"setup-environment-ini":{
				"action":"push-template",
				"source":"project/config.ini",
				"target":"/home/%(user)s/%(domain)s/source/%(tag)s/config/config.ini",
				"params":{
					"DB_DATABASE":"yourproject_db",
					"DB_PASSWORD":"",
					"DB_USER":"root"
				}
			},
			"setup-environment-php":{
				"action":"push-template",
				"source":"project/config.php",
				"target":"/home/%(user)s/%(domain)s/source/%(tag)s/config/config.php",
				"params":{
					"DB_DATABASE":"yourproject_db",
					"DB_PASSWORD":"",
					"DB_USER":"root"
				}
			},
			"setup-environment-python":{
				"action":"push-template",
				"source":"project/config.py",
				"target":"/home/%(user)s/%(domain)s/source/%(tag)s/config/config.py",
				"params":{
					"DB_DATABASE":"yourproject_db",
					"DB_PASSWORD":"",
					"DB_USER":"root"
				}
			},
			"ls":{
				"action":"execute-command",
				"command":"ls -las /home/%(user)s/%(domain)s/source/%(tag)s"
			}
			
		},
		"after_mysql_backup":{
			"ls":{
				"action":"execute-command",
				"command":"ls -las /home/%(user)s/%(domain)s/db_backup/"
			}
		}
	}
}